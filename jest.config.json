{
    "bail": 1,
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
        "node_modules",
        "<rootDir>/src/server.ts"
    ],
    "moduleFileExtensions": [
        "ts",
        "js",
        "json",
        "node"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node"
}